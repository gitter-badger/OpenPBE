FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      T.oil;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 302.15;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 302.15;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.oil;
        inletValue      uniform 302.15;
        value           uniform 302.15;
    }
    walls
    {
        type            zeroGradient;
    }
    symmetry
    {
        type            symmetryPlane;
    }
}


