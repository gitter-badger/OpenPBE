FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon.oil;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 1.23e-04;

boundaryField
{
    outlet
    {
        type            inletOutlet;
        phi             phi.oil;
        inletValue      uniform 1.23e-04;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1.23e-04;
    }
    walls
    {
        type            epsilonWallFunction;
        value           uniform 1e-10;
    }
    front
    {
        type wedge;
    }
    back
    {
        type wedge;
    }
    axis
    {
       type      symmetryPlane;
    }
}


